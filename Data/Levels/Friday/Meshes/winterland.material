material WinterBorder_mat : CommonShaderNmOcc
{
	set_texture_alias DiffuseMap WinterBorder_Colour.tga
	set_texture_alias OcclusionMap Occlusion_Winterland.tga
	set_texture_alias NormalMap WinterBorder_Normal.tga


	technique
	{
		pass
		{
			specular 1.0 1.0 1.0 1.0  2
		}
	}
}
