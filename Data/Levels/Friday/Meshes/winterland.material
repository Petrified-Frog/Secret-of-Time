material wl_StreetProps_mat
{
	technique
	{
		pass
		{

			texture_unit
			{
				texture StreetProps.tga
				tex_coord_set 0
				colour_op modulate
				scale 1 1
				scroll 0 0
				rotate 0
			}
		}
	}
}
